{
    "manifest_version": 2,
    "name": "EzNote",
    "description": "EasyNote Hackathon",
    "version": "1.0.0",

    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_Title": "EasyNote"
    
    },
   
    "permissions": [
    "tabs", 
    "https://www.google.com/*",
    "storage"
]

 
}